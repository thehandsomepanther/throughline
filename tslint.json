{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js",
      "public/worker.js",
      "src/registerServiceWorker.js"
    ]
  },
  "rules": {
    "interface-name": [
      true,
      "never-prefix"
    ],
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false,
    "no-empty": false,
    "no-console": false
  }
}